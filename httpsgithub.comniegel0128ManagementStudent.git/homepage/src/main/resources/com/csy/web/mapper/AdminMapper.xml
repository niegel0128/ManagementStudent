<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csy.web.mapper.AdminMapper">
   <insert id="insert"></insert>
   <select id="login" resultType="com.csy.web.admin.AdminBean" parameterType="com.csy.web.admin.AdminBean">
    SELECT * FROM admin
    WHERE id = #{aId} AND password = #{aPassword}   
   </select>
   <select id="selectAll" resultType="com.csy.web.admin.AdminBean">
     SELECT * FROM admin  
   </select>
   <select id="selectByName">
   	SELECT * FROM admin
   	WHERE name = #{aName} 
   </select>
   <select id="selectById">
   	SELECT * FORM admin
   	WHERE id = #{aId}    
   </select>
   <select id="count">
	SELECT count(*) FROM admin       
   </select>
   <update id="update">
       
   </update>
   <delete id="delete"></delete>
</mapper>							